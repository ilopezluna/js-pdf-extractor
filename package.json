{
  "name": "pdf-data-extractor",
  "version": "1.0.4",
  "description": "A TypeScript library to extract structured data from PDFs using JSON schemas and OpenAI API compatible models.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "pdf",
    "extraction",
    "openai",
    "json-schema",
    "typescript"
  ],
  "author": "Ignacio LÃ³pez Luna <ignasi.lopez.luna@gmail.com>",
  "homepage": "https://github.com/ilopezluna/js-pdf-extractor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilopezluna/js-pdf-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/ilopezluna/js-pdf-extractor/issues"
  },
  "license": "MIT",
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "USAGE.md"
  ],
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.0",
    "@types/pdf-parse": "^1.1.5",
    "jest": "^30.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "openai": "^6.2.0",
    "pdf-parse": "^2.2.2",
    "pdf-to-img": "^5.0.0"
  }
}
